<%= form_with(scope: :session, url:login_path, local: true) do |form| %>
  <p>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </p>
  <p>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </p>

  <div class="actions">
    <%= form.submit('Log in')%>
  </div>
<% end %>
